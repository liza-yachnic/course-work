<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="678.0" style="-fx-background-color: #fafafa;" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.controllers.SignUpController">
    <Label layoutX="222.0" layoutY="14.0" prefHeight="64.0" prefWidth="250.0" text="Регистрация" textFill="#7449ba">
        <font>
            <Font name="FreeSans Bold" size="43.0"/>
        </font>
    </Label>
    <TextField fx:id="fioField" layoutX="36.0" layoutY="86.0" prefHeight="24.0" prefWidth="300.0" promptText="ФИО"
               style="-fx-background-color: #fafafa; -fx-border-color: silver; -fx-border-radius: 5;">
        <font>
            <Font size="19.0"/>
        </font>
    </TextField>
    <TextField fx:id="phoneNumberField" layoutX="358.0" layoutY="86.0" prefHeight="24.0" prefWidth="300.0"
               promptText="Номер телефона"
               style="-fx-background-color: #fafafa; -fx-border-color: silver; -fx-border-radius: 5;">
        <font>
            <Font size="19.0"/>
        </font>
    </TextField>
    <PasswordField fx:id="passwordField" layoutX="358.0" layoutY="144.0" prefHeight="24.0" prefWidth="300.0"
                   promptText="Пароль"
                   style="-fx-background-color: #fafafa; -fx-border-color: silver; -fx-border-radius: 5;">
        <font>
            <Font size="19.0"/>
        </font>
    </PasswordField>
    <Button fx:id="signUpButton" layoutX="246.0" layoutY="290.0" mnemonicParsing="false" onAction="#clickOnSingUpBtn"
            prefHeight="44.0" prefWidth="202.0" style="-fx-background-color: #eb3480;" text="Зарегистрироваться"
            textFill="WHITE">
        <font>
            <Font name="Dyuthi Regular" size="19.0"/>
        </font>
    </Button>
    <TextField fx:id="mailField" layoutX="36.0" layoutY="204.0" prefHeight="24.0" prefWidth="300.0"
               promptText="Ваш email"
               style="-fx-background-color: #fafafa; -fx-border-color: silver; -fx-border-radius: 5;">
        <font>
            <Font size="19.0"/>
        </font>
    </TextField>
    <TextField fx:id="loginField" layoutX="36.0" layoutY="144.0" prefHeight="24.0" prefWidth="300.0" promptText="Логин"
               style="-fx-background-color: #fafafa; -fx-border-color: silver; -fx-border-radius: 5;">
        <font>
            <Font size="19.0"/>
        </font>
    </TextField>
    <TextField fx:id="passportIdField" layoutX="358.0" layoutY="204.0" prefHeight="24.0" prefWidth="300.0"
               promptText="Номер паспорта"
               style="-fx-background-color: #fafafa; -fx-border-color: silver; -fx-border-radius: 5;">
        <font>
            <Font size="19.0"/>
        </font>
    </TextField>
    <Label fx:id="errorLabel" alignment="CENTER" layoutX="106.0" layoutY="256.0" prefHeight="26.0"
           prefWidth="482.0" textFill="RED"/>
    <Button fx:id="closeSignUpButton" layoutX="554.0" layoutY="342.0" mnemonicParsing="false" onAction="#clickOnCloseSignUpBtn"
            prefHeight="44.0" prefWidth="102.0" style="-fx-background-color: #eb3480;" text="Выйти" textFill="WHITE">
        <font>
            <Font name="Dyuthi Regular" size="19.0"/>
        </font>
    </Button>
</AnchorPane>
